{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": ["**/models/*.ts"],
  "rules": {
    "semi": "off",
    "no-unused-vars": "off",
    "indent": ["error", 2]
  }
}
